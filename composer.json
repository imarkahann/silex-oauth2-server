{
    "name": "imarkahann/silex-oauth2-server",
    "description": "A Silex service provider for implementing an OAuth 2 server, using the league/oauth2-server library.",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/imarkahann/silex-oauth2-server"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "league/oauth2-server": "dev-develop",
        "doctrine/dbal": "2.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpunit/dbunit": ">=1.2"
    },
    "autoload": {
        "psr-0": {
            "OAuth2Server": "src/"
        }
    }
}
